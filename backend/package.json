{
    "name": "backend",
    "version": "1.0.0",
    "type": "module",
    "description": "Stampee Backend API",
    "main": "dist/index.js",
    "scripts": {
        "dev": "nodemon --watch src --ext ts --exec tsx src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@fastify/cors": "^9.0.1",
        "@fastify/multipart": "^8.3.0",
        "fastify": "^4.28.1"
    },
    "devDependencies": {
        "@types/node": "^22.5.5",
        "@typescript-eslint/eslint-plugin": "^8.5.0",
        "@typescript-eslint/parser": "^8.5.0",
        "eslint": "^9.10.0",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "tsx": "^4.19.1",
        "typescript": "^5.6.2",
        "vitest": "^2.1.1"
    }
}